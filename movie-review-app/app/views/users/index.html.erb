<%= render 'menu' %>
        
<h1>Listing users</h1>
 

<h4>
<table>
  <tr>
    <th>Username</th>
    <th>Encrypted password</th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.password_digest %></td>
      <td><%= link_to 'Destroy',
                                user_path(user),
                                method: :delete,
                                data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>


<%= render 'footer' %>